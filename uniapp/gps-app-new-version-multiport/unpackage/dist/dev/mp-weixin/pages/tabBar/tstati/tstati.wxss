
page {
    min-height: 100%;
    /* height: auto; */
    height: 100%;
}

/* 解决头条小程序字体图标不显示问题，因为头条运行时自动插入了span标签，且有全局字体 */
.uni-icon {
    font-family: uniicons;
    font-weight: normal;
}
.uni-container {
    padding: 15px;
}
.uni-header-logo {
    padding: 15px 15px;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
            flex-direction: column;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
            justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    margin-top: 10rpx;
}
.uni-header-image {
    width: 80px;
    height: 80px;
}
.uni-hello-text {
    margin-bottom: 20px;
}
.hello-text {
    color: #7A7E83;
    font-size: 14px;
    line-height: 20px;
}
.hello-link {
    color: #7A7E83;
    font-size: 14px;
    line-height: 20px;
}
.uni-panel {
    margin-bottom: 12px;
}
.uni-panel-h {
    background-color: #ffffff;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    padding: 12px;
}
/*
.uni-panel-h:active {
    background-color: #f8f8f8;
}
 */
.uni-panel-h-on {
    background-color: #f0f0f0;
}
.uni-panel-text {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    color: #000000;
    font-size: 14px;
    font-weight: normal;
}
.uni-panel-icon {
    margin-left: 15px;
    color: #999999;
    font-size: 14px;
    font-weight: normal;
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
    -webkit-transition-duration: 0s;
            transition-duration: 0s;
    -webkit-transition-property: -webkit-transform;
    transition-property: -webkit-transform;
    transition-property: transform;
    transition-property: transform, -webkit-transform;
}
.uni-panel-icon-on {
    -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
}
.uni-navigate-item {
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
            flex-direction: row;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
    background-color: #FFFFFF;
    border-top-style: solid;
    border-top-color: #f0f0f0;
    border-top-width: 1px;
    padding: 12px;
}
.uni-navigate-item:active {
    background-color: #f8f8f8;
}
.uni-navigate-text {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
    color: #000000;
    font-size: 14px;
    font-weight: normal;
}
.uni-navigate-icon {
    margin-left: 15px;
    color: #999999;
    font-size: 14px;
    font-weight: normal;
}
.tstati-container {
  height: 100%;
  padding: 0 48rpx;
  background: #f7f7f7;
}
.tstati-container .collapse-container {
  background: none;
}
.tstati-container .collapse-container .uni-list {
  text-align: center;
}
.tstati-container .collapse-container .uni-list .uni-list-cell {
  width: 100%;
  font-size: 28rpx;
  text-align: center;
}
.tstati-container .collapse-container .uni-list .uni-list-cell > view {
  padding: 50rpx 0 30rpx 0;
  border-bottom: 1px solid #efefef;
}
.tstati-container .collapse-container .uni-list .uni-list-cell > view > view {
  display: inline-block;
}
.tstati-container .collapse-container .uni-list .uni-list-cell > view > view.text {
  padding-top: 4rpx;
}
.tstati-container .collapse-container .uni-tip-group-button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 50rpx 0rpx 40rpx 0;
}
.tstati-container .collapse-container .uni-tip-group-button .uni-tip-button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  color: #3b4144;
  font-weight: 600;
}
.tstati-container .collapse-container .uni-tip-group-button .uni-tip-button view {
  height: 64rpx;
  line-height: 64rpx;
  width: 168rpx;
  margin: auto;
  text-align: center;
  letter-spacing: 0;
}
.tstati-container .collapse-container .uni-tip-group-button .uni-tip-button .btn-confirm {
  color: #fff;
  background: #bd0202;
  border-radius: 32rpx;
  box-shadow: 0rpx 10rpx 10rpx 0rpx #d54048;
}
.tstati-container .collapse-container .uni-tip-group-button .uni-tip-button .btn-cancel {
  color: #bdbdbd;
}
.tstati-container .uni-collapse::before,
.tstati-container .uni-collapse::after {
  display: none;
}
.tstati-container .uni-collapse .uni-collapse-cell__title {
  background-color: #fff;
  box-shadow: 0 -2rpx 2rpx 0rpx #efefef inset;
  border-radius: 16rpx;
  height: 114rpx;
  width: calc(100% + 2rpx);
}
.tstati-container .uni-collapse .uni-collapse-cell__content {
  padding: 0 35rpx;
  box-sizing: border-box;
  border: none;
  border-radius: 0 0 16rpx 16rpx;
}
.tstati-container .uni-collapse .uni-collapse-cell {
  margin-top: 40rpx;
  box-sizing: border-box;
  border-radius: 16rpx;
}
.tstati-container .uni-collapse .uni-collapse-cell::after {
  display: none;
}
.tstati-container .uni-collapse .uni-collapse-cell--open {
  background: #fff;
  box-shadow: 0 0 6rpx 2rpx #efefef;
}
/* 提示窗口 */
.uni-tip {
  padding: 30rpx;
  width: 685rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 20rpx;
}
.uni-tip .uni-tip-title {
  text-align: center;
  font-weight: bold;
  font-size: 32rpx;
  color: #333;
}
.uni-tip .uni-tip-content {
  padding: 30rpx;
  font-size: 28rpx;
  color: #666;
  text-align: center;
}
.uni-tip .content > view {
  padding: 30rpx 0;
}
.uni-tip .uni-tip-group-button {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.uni-tip .uni-tip-group-button .uni-tip-button {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #2a4144;
  font-weight: 600;
}
.uni-tip .uni-tip-group-button view:nth-child(2) {
  color: #dc1110;
}

