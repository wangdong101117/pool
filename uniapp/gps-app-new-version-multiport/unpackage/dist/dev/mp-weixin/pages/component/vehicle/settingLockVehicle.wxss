/* 提示窗口 */
.uni-tip {
  padding: 40rpx 30rpx 60rpx 30rpx;
  width: 656rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 16rpx;
  color: #333;
}
.uni-tip .uni-tip-title {
  text-align: center;
  font-weight: bold;
  font-size: 34rpx;
  color: #333;
}
.uni-tip .uni-tip-content {
  padding: 30rpx;
  font-size: 28rpx;
  color: #666;
  text-align: center;
}
.uni-tip .uni-list {
  text-align: center;
}
.uni-tip .uni-list .uni-list-cell {
  width: 100%;
  font-size: 28rpx;
  text-align: center;
}
.uni-tip .uni-list .uni-list-cell > view {
  padding: 50rpx 0 30rpx 0;
  border-bottom: 1px solid #efefef;
}
.uni-tip .uni-list .uni-list-cell > view > view {
  display: inline-block;
}
.uni-tip .uni-list .uni-list-cell > view > view.text {
  padding-top: 4rpx;
}
.uni-tip .uni-list .lock-vehicle-date {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 30rpx 0 20rpx 0;
  border-bottom: 1px solid #ccc;
  box-sizing: border-box;
  font-size: 28rpx;
}
.uni-tip .uni-list .lock-vehicle-date .label-text {
  text-align: left;
  height: 48rpx;
  line-height: 48rpx;
}
.uni-tip .uni-list .lock-vehicle-date .select-date {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 48rpx;
  line-height: 48rpx;
}
.uni-tip .uni-list .lock-vehicle-date .select-date view {
  width: 100%;
  font-size: 28rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
}
.uni-tip .uni-tip-group-button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 8rpx;
  margin-top: 20rpx;
}
.uni-tip .uni-tip-group-button .uni-tip-button {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  color: #3b4144;
  font-weight: 600;
}
.uni-tip .uni-tip-group-button .uni-tip-button view {
  height: 64rpx;
  line-height: 64rpx;
  width: 168rpx;
  margin: auto;
  text-align: center;
  letter-spacing: 0;
}
.uni-tip .uni-tip-group-button .uni-tip-button .btn-confirm {
  color: #fff;
  background: #bd0202;
  border-radius: 32rpx;
  box-shadow: 0rpx 10rpx 10rpx 0rpx #d54048;
}
.uni-tip .uni-tip-group-button .uni-tip-button .btn-cancel {
  color: #bdbdbd;
}

